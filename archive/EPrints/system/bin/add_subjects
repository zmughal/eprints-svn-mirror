#!/usr/bin/perl

use DBI;
use EPrintSite::SiteInfo;
use EPrints::Database;
use EPrints::Session;
use EPrints::Subject;
use EPrints::Log;
use strict;

my $session = EPrints::Session->new( 1 );

my $success = EPrints::Subject->create_subject_table( $session );

if( $success )
{
	print "Inserted subject categories\n";
}
else
{
	my $error = $session->{database}->error();
	print "DB Error: $error\n";
}

$session->terminate();
