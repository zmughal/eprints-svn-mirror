use EPrints;

use strict;

use EPrints::Interface::Processor;

my $session = EPrints::Session->new();

EPrints::Interface::Processor->process( session=>$session, url=>$session->get_repository->get_conf( "perl_url" )."/users/control" );

$session->terminate;

