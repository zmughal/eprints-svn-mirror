<?xml version="1.0" ?>

<!-- 
	Subject citation.
-->

<ep:citation
        xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ep="http://eprints.org/ep3/citations">

<ep:linkhere><span class="ep_subject"><ep:choose>
  <ep:when test="$item.is_set('name')">@name@</ep:when>
  <ep:otherwise>[subjectid=@subjectid@]</ep:otherwise>
</ep:choose>></span></ep:linkhere>

</ep:citation>
