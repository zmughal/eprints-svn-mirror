<?xml version="1.0" ?>

<!-- 
	Citation Styles for user records.
-->

<ep:citation
        xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ep="http://eprints.org/ep3/citations">

<ep:linkhere><span class="ep_name_citation"><ep:choose>
  <ep:when test="$item.is_set('name')">@name@</ep:when>
  <ep:otherwise>No name, email=@email@</ep:otherwise>
</ep:choose></span></ep:linkhere>

</ep:citation>
