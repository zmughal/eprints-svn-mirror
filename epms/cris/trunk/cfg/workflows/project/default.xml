<?xml version="1.0" encoding="utf-8"?>

<workflow xmlns="http://eprints.org/ep3/workflow" xmlns:epc="http://eprints.org/ep3/control">

  <flow>
    <stage ref="default"/>
  </flow>
   
  <stage name="default">
      <component><field ref="uri" /></component>
      <component><field ref="title" /></component>
      <component><field ref="acronym" /></component>
      <component><field ref="contributors" /></component>
      <component><field ref="org_units" /></component>
      <component><field ref="description" /></component>
      <component show_help="always" ><field ref="value" /></component>
      <component><field ref="keywords" /></component>
      <component><field ref="start" /></component>
      <component><field ref="end" /></component>
<!--      <component><field ref="relation" /></component> -->
  </stage>
</workflow>
