######################################################################
#
#  CogPrints search page redirector
#
######################################################################
#
#  $Id$
#
######################################################################

use EPrints::Session;
use EPrints::HTMLRender;

use EPrintSite::SiteInfo;

use strict;

my $session = new EPrints::Session();

$session->{render}->redirect( $EPrintSite::SiteInfo::server_perl . "/search" );

$session->terminate();
